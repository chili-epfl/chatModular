<?xml version="1.0" encoding="utf-8"?>
<grammar xml:lang="en-US" version="1.0" root="root"
	xmlns="http://www.w3.org/2001/06/grammar" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.w3.org/2001/06/grammar http://www.iristk.net/xml/srgs.xsd" tag-format="semantics/1.0">

  <rule id="root" scope="public">
      <one-of>
          <item>one<tag>out.number=1</tag></item>
          <item>two<tag>out.number=2</tag></item>
          <item>three<tag>out.number=3</tag></item>
          <item>four<tag>out.number=4</tag></item>
          <item>five<tag>out.number=5</tag></item>
          <item>six<tag>out.number=6</tag></item>
          <item>seven<tag>out.number=7</tag></item>
          <item>eight<tag>out.number=8</tag></item>
          <item>nine<tag>out.number=9</tag></item>
          <item>
          	<one-of>
         		<item>ten</item>
				<item>it's ten</item>
		  	</one-of>
		  <tag>out.number=10</tag>	
		  </item>
          <item>eleven<tag>out.number=11</tag></item>
          <item>twelve<tag>out.number=12</tag></item>
          <item>thirteen<tag>out.number=13</tag></item>
          <item>fourteen<tag>out.number=14</tag></item>
          <item>fifteen<tag>out.number=15</tag></item>
          <item>sixteen<tag>out.number=16</tag></item>
          <item>seventeen<tag>out.number=17</tag></item>
          <item>eighteen<tag>out.number=18</tag></item>
          <item>nineteen<tag>out.number=19</tag></item>
          <item>twenty<tag>out.number=20</tag></item>
          
          <item>
			<one-of>
				<item>yes</item>
				<item>yes please</item>
				<item>sure</item>
				<item>I'd like that</item>
			</one-of>
			<tag>out.yes=1</tag>	
			</item>
           <item>
			<one-of>
				<item>no</item>
				<item>no thanks</item>
				<item>I got it</item>
				<item>no I'm fine</item>
			</one-of>
			<tag>out.no=1</tag>	
			</item>
          
          <item>
			<one-of>
				<item>I don't know</item>
				<item>I need help</item>
				<item>I can't do it alone</item>
			</one-of>
			<tag>out.dontknow=1</tag>	
			</item>
		
			<item>
			<one-of>
				<item>Florida</item>
				<item>New York</item>
				<item>France</item>
			</one-of>
			<tag>out.place=1</tag>	
			</item>
			
			<item>
			<one-of>
				<item>restaurant</item>
				<item>hotel</item>
				<item>holiday</item>
			</one-of>
			<tag>out.noun=1</tag>	
			</item>
      </one-of>
  </rule>
  
</grammar>
